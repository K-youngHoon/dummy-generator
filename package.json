{
  "name": "nodejs-file-generator",
  "version": "1.0.0",
  "main": "generator.js",
  "scripts": {
    "package": "pkg .",
    "build": "pkg dummygen.js --targets node18-win-x64",
    "start": "node dummygen.js"
  },
  "pkg": {
    "scripts": "generator.js",
    "targets": [
      "node18-win-x64"
    ],
    "assets": [
      "node_modules/sharp/build/Release/*",
      "node_modules/sharp/vendor/lib/*"
    ],
    "outputPath": "dist"
  },
  "license": "ISC",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/K-youngHoon/dummy-generator.git"
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/K-youngHoon/dummy-generator/issues"
  },
  "homepage": "https://github.com/K-youngHoon/dummy-generator#readme",
  "dependencies": {
    "exceljs": "^4.4.0",
    "inquirer": "^8.2.7",
    "pkg": "^5.8.1",
    "sharp": "^0.34.5"
  }
}
